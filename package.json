    "lodash": "4.17.21",
    "next": "11.0.1",
    "next-api-middleware": "0.7.1",
    "next-seo": "4.24.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.6.4",
    "react-query": "3.16.0",
    "react-transition-group": "4.4.1",
