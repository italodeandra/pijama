{
  "name": "@italodeandra/pijama",
  "version": "0.6.0",
  "description": "Majapi's framework for building apps",
  "private": true,
  "main": "index.js",
  "typings": "index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "prepare": "husky install",
    "prebuild:lib": "shx rm -rf dist",
    "build:lib": "tsc --build lib/tsconfig.json",
    "postbuild:lib": "shx cp package.json readme.md jest.config.js tsconfig.json next.config.js .eslintrc babel.config.js .lintstagedrc.js .prettierrc.js dist",
    "pre-publish": "npm run build:lib && cd dist && json -I -f package.json -e \"this.private=false; this.devDependencies=undefined; this.scripts=undefined;\"",
    "publish:github": "npm run pre-publish && cd dist && npm publish",
    "publish:yalc": "npm run pre-publish && cd dist && yalc publish --private",
    "dev": "start-storybook -p 6006",
    "build": "build-storybook",
    "start": "http-server storybook-static",
    "test": "jest",
    "coverage": "jest --coverage",
    "pre-commit": "lint-staged",
    "pre-push": "npm test"
  },
  "dependencies": {
    "@emotion/cache": "11.4.0",
    "@emotion/react": "11.4.0",
